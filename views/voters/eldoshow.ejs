<!DOCTYPE html>
<html>

<head>
  <% include ../partials/head %>
    <meta charset="utf-8">
    <title></title>
</head>

<body>
  <div class="jumbotron" id="fortop" align="center">
    <h1 style="color:white;">Voters</h1>
  </div>
  <h2 align="middle" style="color:#ff0000"> District </h2>
  <div class="container">
    <div class="well">Click on District Name for more details</div>
  </div>
  <div class="container">
    <div class="well">Number of Seats for this election: <%= elections[0].seats%></div>
  </div>


  <div class="row">
    <div class="col-sm-2">

    </div>
    <div class="col-sm-8">
      <div class="well"> Districts in Election  </div>
      <table class="table table-hover">
        <thead>
          <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Eid</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i=0;i<regs.length;i++) {%>
          <tr>
            <td><%= regs[i].id%></td>
            <td>  <a href="/voters/elections/district/ooshow/<%=regs[i]._id%>/<%= elections[0].eid%> "> <%= regs[i].name%></a></td>
            <td><%= regs[i].eid%></td>
          </tr>
          <% } %>
        </tbody>

      </table>

    </div>

    <div class="col-sm-2">

    </div>




  </div>



  <br />





</body>

</html>
